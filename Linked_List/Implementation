#include <iostream>
using namespace std;
class node{
    public:
    int data;
    node*next;

    node (int data){
    this->data=data;
    next='\0';
    }

};
void print(node *head){
    node*temp=head;
    while(temp!='\0'){
        cout<<temp->data<<" ";
        temp=temp->next;
    }
    delete temp;
}
node * take_input(){
     int data;
     cin>>data;
     node*head='\0';
     node*tail='\0';
     while(data!=-1){
        node *newnode=new node(data);
        if(head=='\0'){
            head=newnode;
            tail=newnode;
        }else{
            tail->next=newnode;
            tail=tail->next;
        }
        cin>>data;
     }
     return head;
}
int main()
{
  cout<<"Enter data, last data should be -1"<<endl;
  node *head=take_input();
  cout<<"Output :"<<endl;
  print(head);
}


**Output Terminal**


Enter data, last data should be -1
5
6
1
3
7
-1
Output :
5 6 1 3 7
Process returned 0 (0x0)   execution time : 14.634 s
Press any key to continue.


